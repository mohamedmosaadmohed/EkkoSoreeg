@{
    ViewBag.pageTitle = "Order Management";
    ViewBag.CardTitle = "View Orders";
}

<section class="section">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <div class="d-flex justify-content-between align-items-center mb-2">
                        <h5 class="card-title">Order Table</h5>
                        <a class="btn btn-success me-3" href="#"><i class="fas fa-download"></i></a>
                    </div>
                    <!-- Default Table -->
                    <table id="mytable" class="table">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Name</th>
                                <th>PhoneNumber</th>
                                <th>orderStatus</th>
                                <th>TotalOrder</th>
                                <th>Email</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
@section Scripts {
    <script src="~/dashboard/js/order.js"></script>
}
